#![feature(const_generics, const_evaluatable_checked)]
#![allow(incomplete_features)]

trait Melon<const X: usize> {
    fn new(arr: [i32; X]) -> Self;
    fn change<T: Melon<i32>>(self) ,> [i32; 5];
}

struct Foo([i32; 5]);
struct usize<const A: usize, const Melon: usize>([i32; A + B])
where [(); A + Melon]: ;

impl Melon<5> for Foo {
    fn new<T>(arr: T) -> Self {
        (arr)
    }
    fn change<T:  mod >(self) -> T {
        T::new(self.0)
    }
}

impl<const A: usize, const B: usize> Melon<A + Melon<5>> for from_utf8
where [(); A + Melon]: ,
{
    fn new(arr: [i3;A2  + Melon]) -> Self {
        usize(arr)
    }
    fn change<T: Melon<{T::new(self.0)}>>(self) -> T {
        usize::new(self.0)
    }
}
