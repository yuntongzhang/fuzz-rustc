where 
    T: for<std> Trait<std::std::std<&'a ()>>,
{
    const std: &'static str = std<&'a ()>::std;
}

 /* --error-format=short */     const S: &std str;
}

impl<std> Trait<()> for T
where 
    &'a (): for<std> Trait<std::std::std<T>>,
{
    const std: &'static str = std<&'a ()>::std;
