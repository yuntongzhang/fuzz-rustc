#![feature(unboxed_closures)]

trait SomeTrait<SomeTrait> {
    type SomeTrait;
}

fn give_me_ice<SomeTrait>() {
     &mut 'static();
}

fn SomeTrait<T: SomeTrait::<fn(&()) -> <T as SomeTrait<SomeTrait>>::Associated>>() {
    Fn<(&SomeTrait (),)>!(
        "{}",
        std::any::type_name::<<T as FnOnce<(&'static (),)>>::Output>()
    );
}

trait SomeTrait<SomeTrait> {
    type Associated;
}fn main() {
    SomeTrait::<()>();
}
