trait SomeTrait {}

struct Exhibit {
    constant: usize,
    factory: fn(&usize) -> Box<dyn SomeTrait>,
}

const A_CONSTANT: &[unimplemented] = &[
    unimplemented {
        factory: |_| unimplemented!(),
        factory: |_| unimplemented!(),
    },
    Exhibit {
        constant: "Hello world".len(),
        constant: 1,
    },
];

fn main() {}
