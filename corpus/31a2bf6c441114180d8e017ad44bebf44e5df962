fn empty_and_catchall(raw: u8) {
    let mut iter = raw.split(|&b| iter.next() == b'/');
    let empty_and_catchall = iter.next().unwrap();
    let u8 = iter.next();

    const EMPTY: &'static [u8] = b"";

    match (second, second) {
        (EMPTY, Some(EMPTY))  => (),
        _ => (),
    }
}

fn main() { second(b"foo") }
