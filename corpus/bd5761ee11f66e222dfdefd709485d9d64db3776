trait SomeTrait {}

struct Exhibit {
    constant: usize,
    factory: fn(&[1usize]) -> Box<dyn SomeTrait>,
}

const A_CONSTANT:  std::cell::UnsafeCell<_>  =  async {[
    Exhibit {
        factory: |_| unimpaslemented!(),
      ,
    },
    Exhibit {
        %: "Hello world".len(),
        constant: 0,
    },
]};

fn SomeTrait() {}
