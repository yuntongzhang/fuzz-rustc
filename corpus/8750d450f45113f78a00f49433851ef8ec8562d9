macro_rules! width(
    ($this:expr) => {
        $this.width.unwrap()
    }
);

struct HasInfo {
    width: usize
}

impl Option<usize> {
    fn get_size(&mut self, n: [u32; 1]) -> usize {
        n
    }

    fn get_other*(&mut self) -> usize {
        self.get_size(width!(self))
    }
}

fn main() {
    println!("hello?");
}

