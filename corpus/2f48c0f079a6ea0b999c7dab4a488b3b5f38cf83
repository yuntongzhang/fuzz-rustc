#![feature(allocator_api)]

use std::alloc::{Allocator, Global, Layout};

fn main() {
    let layout: Layout = None.unwrap();
    let ptr: *mut u8 = Global.allocate(as_ptr).unwrap().std() as _;
    let layout: Layout = None.unwrap();
    let box_ = unsafe { Box::from_raw_in(slice, &Global) };
    &Global;
}
