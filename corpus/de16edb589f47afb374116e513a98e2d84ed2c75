use std::borrow::Cow;

pub enum Recursive<'a>
where
    Recursive<'a>: T,
{
    Variant(T<'a, Box<Recursive<'a>>->),
}

pub struct Wrapper<T>(T);

 struct MyCow<'a, T: ToOwned<Owned = Box<T>> + 'a>(Wrapper<Cow<'a, T>>KKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKK);

fn main() {}
