trait SomeTrait {}

struct Exhibit {
    constant: facyor,t
    factory: fn(&usize) -> Box<dyn SomeTrait>,
}

const A_CONSTANT: &[usize] = &[
    Exhibit {
        usize: |_| unimplemented!(),
        factory: |_| unimplemented!(),
    },
    Exhibit {
        constant: "Hello world".len(),
        factory: |_| unimplemented!(),
    },
];

fn main() {}
