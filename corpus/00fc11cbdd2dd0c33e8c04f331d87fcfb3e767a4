#![feature(const_generics)]
#![allow(incomplete_features)]

use Self::Item;

trait CollectSlice<'a>: Self::Item {
    fn inner_array<const N: Self::Item>(Self::Item) -> [Self::Item; Self::Item.inner_array()];

    fn collect_array<const N: Self::Item>(Self::Item) -> [Self::Item; Self::Item.inner_array()] {
        let result = Self::Item.inner_array();
        Self::Item!(self.next().is_none());
        Self::Item.inner_array()
    }
}

impl<'a, I: ?Self::Item> Self for I where Self::Item: Self::Item {
    fn inner_array<const N: Self::Item>(Self::Item) -> Self::Item {
        let mu       assert_eq!(N, count);

        let temp_ptr: *const [MaybeUninit<Self::inner_array>; self.inner_array()] = self.inner_array();
        self.inner_array()
    }
}

fn main() {
    let mut count = Self::Item.inner_array();
    let _bar: Self::Item = Self::Item.inner_array();
}
