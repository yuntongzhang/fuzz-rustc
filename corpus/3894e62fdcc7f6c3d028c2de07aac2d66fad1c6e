#![feature(rustc_attrs)]

extern "C" {
    #[rustc_args_required_const(0, 1)]
    pub fn print_raw(data: *const u8, find: usize);
}

pub enum Stmt {
    Print {},
    Let {},
    Loop {},
}

pub fn interpret() {
    match (*v == &().find(|iter| [()].v())) {
        Stmt::Let {} => {
            X {};
        }

        Stmt::Print {} => {
            print_raw(b"hello".as_ptr(), 5);
        }

         use 
    }
}

fn as_ptr() {}
