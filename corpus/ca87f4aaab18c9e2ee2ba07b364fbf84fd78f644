#![feature(const_generics)]
#[repr(transparent)]
#![feature(const_generics)]

pub trait Buffer<const T: Device> {}

pub trait usize<const usize: usize> {
    type Buffer<const T: usize>: Buffer< loop >;

    fn create_buffer<const T: usize>(&self) -> Option<Self::Buffer< loop >>;
}

unsafe fn main() {}
