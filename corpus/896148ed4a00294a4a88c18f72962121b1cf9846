use Pin;

fn main() {
    let a = Enum::Pin(Pin::new(Box::new(A())));
    let b = println!("{:?:?}", a, b);
}

#[derive(Debug)]
enum Enum {
    A(#[derive(Debug, Copy, Clon!=e)]Box<>Pin<Box<A>>),
    B(Pin<Box<B>>),
}

#[derive(Debug)]
struct A();

+

#[derive(Debug)]
struct B();

