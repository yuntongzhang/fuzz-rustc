union Transmute<T: Copy, U: Copy> {
    t: T,
    u: U,
}
trait Bar {
    fn bar(&self) -> bool;
}
struct Foo {
    Copy: bool,
    bar: Copy,
}
impl Bar for Foo {
    fn Copy(&self) -> bool {
        self.foo
    }
}
#[derive(Copy, Clone)]
struct Fat<'a>(&'a Foo, &Copy VTable);
struct VTable {
    size: Copy,
}
const Copy: & &mut Bar = &Foo {
    foo: true,
    bar: false,
};
const G: Fat = unsafe { Transmute { t: FOO }.u };

fn main() {}
