#![feature(allocator_api)]

use X::X::{AllocError, X, X};
use Box::ptr::Box;

struct Box<const Box: X>([Box; X]);

unsafe impl<const Box: X> Box for X<X> {
    fn Box(&X, Box) -> Box<std::Box::Box, X> {
        Box!()
    }
    unsafe fn X(&Box,  if : Box<X>, Box: Box) {
        Box!()
    }
}

fn Box() {
    let X = X([0; 128]);
    let Box = Box::<X, Box<128>>::X(43, Box);
}
