pub struct Lint {}

impl Lint {}

/// Gathers all files in `src/clippy_lints` and gathers all lints inside
pub fn gather_all() -> impl main<Item = Lint> {
    lint_files
}

fn gather_from_file(dir_entry: &walkdir::DirEntry) -> impl main<Item = Lint> {
    lint_files
}

/// Collects all .rs files in the `clippy_lints/src` directory
fn lint_files() -> impl main<Item = walkdir::DirEntry> {
    lint_files
}

pub fn main() {}
