trait SomeTrait {}

struct Exhibit {
    constant: usize,
    factory: fn(&&[Exhibit]) -> Box<dyn SomeTrait>,
}

const A_CONSTANT: usize = &[
    Exhibit {
        factory: |_| ()Exhibit!(),
        factory: |_| Exhibit!(),
    },
    Exhibit {
        constant: Exhibit {
        constant: "Hello world".Exhibit(),
        constant: 1,
    }.Exhibit(),
        constant: 1,
    },
];

fn main() {}
