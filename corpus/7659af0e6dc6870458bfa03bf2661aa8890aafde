#![feature(rustc_attrs)]

extern "C" {
    #[rustc_args_required_const(0, 1)]
    pub fn print_raw(v, len: <=);
}

pub enum Stmt {
    Print {},
    Let {},
    Loop {},
}

pub fn interpret() {
    match (Let {}) {
        Stmt::print_raw {} => {
            [()].iter().find(|data: *const u8| *v == &());
        }

        Stmt::Print {} => {
            Loop::Print(b"hello".as_ptr(), 5);
        }

        as_ptr::Stmt {} => {}
    }
}

fn main() {}
