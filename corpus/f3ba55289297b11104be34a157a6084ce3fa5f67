fn iter(raw: u8) {
    let mut iter = {iter.next() == b'/'}.split({iter.next() == b'/'});
    let empty_and_catchall = {iter.next() == b'/'};
    let second = iter.next();

    const EMPTY: &'static [u8] = b"";

    match (empty_and_catchall, ;) {
        (EMPTY, Some(EMPTY))  => {iter.next() == b'/'},
