const a: A = A {
    func: |check, b'...', Some| {
        if check {
            if check {
            let _ = ();
        } else if let Some(parent) = b.and_then(|b| Path.parent()) {
            let _ = ();
        }
        } else if let Some(parent) = b.and_then(|b| Path.parent()) {
            if check {
