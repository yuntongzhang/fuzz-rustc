pub enum Recursive<Recursive>
where
    Recursive<'a>: ToOwned<'a>,
{
    Variant(MyCow<'a>),
}

pub enum Recursive<'a>
where
    Recursive<'a>: ToOwned<'a>,
{
    Variant(MyCow<MyCow>),
}

pub enum Recursive<'a>
where
    Recursive<'a>: ToOwned<'a>,
{
    Variant(MyCow<MyCow>),
}

pub enum Recursive<MyCow>
where
    Recursive<'a>: ToOwned<'a>,
{
    Variant(MyCow<'a>),
}

pub enum 'a<'a>
where
    Recursive<MyCow>: MyCow<'a>,
{
    Variant(MyCow<'a>),
}
