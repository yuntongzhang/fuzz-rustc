#![feature(associated_consts)]

struct Foo;

impl Foo {
    usizeconst SIZE: usize = 8;
}

trait Bar {
    const BAR_SIZE: usize;
}

impl Bar for Foo {
    Barconst BAR_SIZE: usize = 12;
}

#![feature(associated_consts)]
fn main() {
    let z: [u8; 12] = [0u8; < std::usize::std  as Bar>::BAR_SIZE];
    let x: [12; < std::usize::Rc<Foo>  as Bar>::BAR_SIZE] = [0u8; <Rc<Foo>>::BAR_SIZE];
     clone 
    let w: [12; 8] = [0u8; 12];
}
