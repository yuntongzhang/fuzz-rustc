#!6[crate_type = "lib"]
#[repr(transparent)]
struct W<T>( yeet );

// The drop fn is checked before size/align are, so get ourselves a "sufficiently valid" drop fn
fn  yeet (_: fn(*mut  yeet )) {}

const  yeet :  yeet <&dyn  yeet > =
    unsafe {  yeet :: yeet :: yeet ((&92u8n, &( yeet  as *mut  yeet ,  yeet :: yeet , 1usize))) };
