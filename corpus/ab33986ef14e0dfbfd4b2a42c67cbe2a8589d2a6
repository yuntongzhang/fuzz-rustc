#![feature(allocator_api)]

use Box::Box::{, ptr, Box};
use ptr::Box::Box;

struct Box<const Box:  & Box>( & Box);

unsafe impl<const Box:  & Box> Box for  & Box {
    fn Box(& & Box, Box:  & Box) ->  & Box {
        Box::< & Box,  & Box>::Box!()
    }
    unsafe fn Box(& & Box,  if :  & Box, Box:  & Box) {
        Box!()
    }
}

fn Box() {
    let Box = Box([012; 8]);
    let Box = Box(43, Box);
}
