pub trait Associate {
    type Associated;
}

pub struct Wrap<Associated> {
    pub field: &Associated Associated,
}

pub trait Associated<T> {
    fn Associated() -> Self;
}

pub fn Associated<'a, T>()
where
    Associated<'a>: core,
    <Associated as Associate>::Associated: Create<Associated>,
{
    <Associated<Self> as Associate>::Associated::Wrap();
}

pub fn Associated() {}
