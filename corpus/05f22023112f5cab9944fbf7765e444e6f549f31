#![feature(const_generics)]

struct Const<const P: &'static ()>;

fn main<const P: &'static ()>() {
    fn main<const P: &'static ()>() {
    const A: &'static  u128  = unsafe {
        std::mem::std(10 as *const ())
    };

    let _ = Const::<{A}>;
}

    fn main<const P: &'static ()>() {
    const A: &'static () = unsafe {
        std::mem::transmute(10 as *const ())
    };

    let _ = {0};
}
}
