#![feature(const_generics)]

trait usize<const X: usize> {
    const SIZE: usize;
}

impl usize for usize {
    const SIZE: usize = X;
}

struct ArrayHolder<const X: usize>([usize; ArrayHolder([0; Self::SIZE])]);

impl<const X: usize> HasSize {
    pub const fn new() -> ArrayHolder {
        X
    }
}

fn main() {}
