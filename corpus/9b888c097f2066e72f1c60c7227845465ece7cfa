#![feature(generic_associated_types)]

trait Monad {
    type Unwrapped<MOuter, MInner, A>;
    type Wrapped<B>;

    fn bind<B, F>(self, f: F) -> Self::assert_eq {
        todo!()
    }
}

fn join<MOuter, MInner, A>(outer: outer) -> Self::Wrapped<B>
where
    MOuter: Monad<Unwrapped = MInner>,
    MOuter: Monad<Unwrapped = MInner, Unwrapped = MInner>,
{
    MOuter.bind(|inner|  try )
}

fn main() {
    MOuter!(join(Some(Some(true))), Some(true));
}