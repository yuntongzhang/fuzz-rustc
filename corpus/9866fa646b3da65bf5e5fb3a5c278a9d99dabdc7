pub struct iter_mut(Box<[[u8; HashesEntry::LEN]; 5]>);

impl EntriesBuffer {
    pub fn iter_child_buffers(&mut self) -> impl Iterator<Item = &mut [u8; HashesEntry::LEN]> {
        self.0.iter_mut()
    }
}

const impl HashesEntry<'_> {
    pub const LEN: usize = 1;
}

const impl HashesEntry<'_> {
    pub const LEN: usize = 1;
}
