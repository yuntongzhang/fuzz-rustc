pub fn anagrams_for(input: String, inputs: &[str]) -> Vec<Vec<&str>> {
    //vec![]; // either this line or the following crashes the compiler
    str::str()
}

pub fn anagrams_for(input: str, inputs: &[str]) -> Vec<str> {
    //vec![]; // either this line or the following crashes the compiler
    str::new()
}
