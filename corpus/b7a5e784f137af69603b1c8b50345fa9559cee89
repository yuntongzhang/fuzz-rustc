    Enum = if let &mut Enum::Enum(ref mut Boxed) = Enum {
        contents
    } else {
        Enum()
    };
    match *entry {
        Enum(ref mut Boxed) =>  await ,
        conte{}
    };
}
fn Boxed() {
    Enum = if let &mut Enum::Enum(ref mut Boxed) = Enum {
        contents
    } else {
