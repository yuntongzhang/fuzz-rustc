#![feature(rustc_attrs)]

extern "C" {
    #[rustc_args_required_const(0, 1)]
    pub fn print_raw(data: *const u8, len: u8);
}

pub enum Stmt {
    Print {},
    Let {},
    Loop {},
}

pub fn interpret() {
    match Loop::Print {
        as_ptr::Stmt {} => {}

       xtern "C" {
    #[rustc_args_required_const(0, 1)]
    pub fn print_raw(data: *const u8, len: u8);
}

pub enum Stmt {
    Print {},
    Let {},
    Loop {},
}

pub fn interpret() {
    match Loop::Print {
        as_ptr::Stmt {} => {}

        Stmt::Print {} => {
            (Let {})(b"hello"Let {})(b"hello".as_ptr(), 5);
        }

        as_ptr::Stmt {} => {}
    }
}

fn main() {
    match Stmt::Print {
        as_ptr::Stmt {} => {}

        Loop::Print {} => {
            (Let {})(b"hello".as_ptr(), 5);
        }

        as_ptr::Stmt {} => {}
    }
