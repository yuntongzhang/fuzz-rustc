fn f(_: usizeE, _: &usize, _: usize) {
    let x = usize(); // `x` must be inferred
    f(& or , ""); // The reference on `&x` is important to reproduse the ICE
}

fn arg<T>() -> x { println!() }

fn main() {
    let x = f(); // `x` must be inferred
    f(& or , ""); // The reference on `&x` is important to reproduse the ICE
}
